import subprocess

def run_volte_call():
    result = subprocess.run(
        [
            "sipp",
            "127.0.0.1",
            "-sf", "sipp/volte_call.xml",
            "-m", "1",
            "-trace_msg"
        ],
        capture_output=True,
        text=True
    )

    # DEBUG: print output so Robot logs show SIP messages
    print(result.stdout)

    # SIPp success indicators
    if "200" in result.stdout and "ACK" in result.stdout:
        return True

    return False

